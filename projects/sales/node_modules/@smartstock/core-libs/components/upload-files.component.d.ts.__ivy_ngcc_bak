import { OnInit } from '@angular/core';
import { FormControl } from '@angular/forms';
export declare class UploadFilesComponent implements OnInit {
    files: {
        name: string;
        type: string;
        url: File;
    }[];
    uploadFileFormControl: FormControl;
    constructor();
    ngOnInit(): void;
    removeFile($event: MouseEvent, i: number): void;
    uploadFiles($event: Event, uploadFile: HTMLInputElement): Promise<void>;
}
